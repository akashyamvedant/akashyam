{
  "version": 2,
  "buildCommand": "pnpm run build:vercel",
  "installCommand": "pnpm install",
  "outputDirectory": "build/client",
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/index.js"
    }
  ]
}
